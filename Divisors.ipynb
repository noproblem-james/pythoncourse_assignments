   "source": [
    "n = int(raw_input('Please enter a number to find its divisors: '))\n",
    "counter = n\n",
    "divisor = n\n",
    "while counter > 0:\n",
    "    if divisor % counter == 0:\n",
    "        print counter\n",
    "    counter -= 1"
