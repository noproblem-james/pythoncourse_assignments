    "user_str = raw_input(\"Please enter a series of numbers separated by commas: \")\n",
    "user_str_no_spaces = user_str.replace(\" \",\"\")\n",
    "user_lst = user_str_no_spaces.split(\",\")\n",
    "lst_a, lst_b = user_lst[::2], user_lst[1::2]\n",
    "print lst_a, lst_b\n",
    "zipped = zip(lst_a, lst_b)\n",
    "print zipped\n",
